type_of_year = input()
holidays = int(input())
weekend_in_home_town = int(input())
all_weekends = 48
weekends_in_sofia = all_weekends - weekend_in_home_town
games_in_sofia = weekends_in_sofia * 3/4
games_in_hometown = weekend_in_home_town
holidays_games = holidays * 2/3
total_games = games_in_sofia + games_in_hometown + holidays_games
if type_of_year == "leap":
    total_games *= 1.15
print(int(total_games))
